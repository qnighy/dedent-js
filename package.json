{
  "name": "@qnighy/dedent",
  "packageManager": "yarn@3.3.1",
  "type": "module",
  "main": "./cjs/dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "require": "./cjs/dist/index.js",
    "import": "./dist/index.js"
  },
  "scripts": {
    "build:babel": "babel -x .ts --ignore 'src/**/*.test.ts' -d dist src",
    "build:babel:cjs": "babel -x .ts --ignore 'src/**/*.test.ts' --config-file ./babel.cjs.config.js -d cjs/dist src",
    "test": "NODE_OPTIONS='--experimental-vm-modules' yarn jest",
    "tsc": "tsc --build"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@jest/globals": "^29.3.1",
    "@types/babel__core": "^7.1.20",
    "@yarnpkg/sdks": "^3.0.0-rc.34",
    "babel-jest": "^29.3.1",
    "jest": "^29.3.1",
    "ts-jest-resolver": "^2.0.0",
    "typescript": "^4.9.4"
  },
  "resolutions": {
    "ts-jest-resolver/jest-resolve": "29"
  },
  "jest": {
    "extensionsToTreatAsEsm": [
      ".ts",
      ".mts",
      ".tsx",
      ".jsx"
    ],
    "resolver": "ts-jest-resolver"
  }
}
